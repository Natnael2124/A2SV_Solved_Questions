class Solution:
    def findValidPair(self, s: str) -> str:
        count=Counter(s)
        lis=[]
        for i in range(len(s)-1):
            if count[s[i]]!=int(s[i]):
                continue

            else:
                if s[i]==s[i+1] or count[s[i+1]]!= int(s[i+1]):
                    continue

                else:
                    lis.append(s[i])
                    lis.append(s[i+1])
                    return "".join(lis)

        return ""
